  "bugs": {
    "url": "https://github.com/kbetlach/chameleon_sheets/issues"
  },
  "homepage": "https://github.com/kbetlach/chameleon_sheets#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "dotenv": "^8.2.0",
    "export-to-csv": "^0.2.1",
    "express": "^4.18.2",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.2",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react-calendar": "^3.0.0",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "toasted-notes": "^3.2.0"
  }
